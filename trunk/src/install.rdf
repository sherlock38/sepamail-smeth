        <em:homepageURL>https://code.google.com/p/sepamail-smeth/</em:homepageURL>
        <em:developer>Ammit Heeramun, idSoft</em:developer>
        <em:developer>Bishan Kumar Madhoo, idSoft</em:developer>
        <em:contributor>Ammit Heeramun, idSoft</em:contributor>
        <em:contributor>Bishan Kumar Madhoo, idSoft</em:contributor>
        <em:contributor>Manfred Sherlock OLM, deciBI/vizGet</em:contributor>
        <em:translator>Ammit Heeramun, idSoft</em:translator>
        <em:translator>Bishan Kumar Madhoo, idSoft</em:translator>
        <em:translator>Manfred Sherlock OLM, deciBI/vizGet</em:translator>
        <!-- Icon for the addon list -->
        <em:iconURL>chrome://{appname}/skin/images/logoSmeth.png</em:iconURL>
        <!-- Icon for the description page -->
        <em:icon64URL>chrome://{appname}/skin/images/logoSmeth.png</em:icon64URL>
